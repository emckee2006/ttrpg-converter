{
    "description":  "Extracted custom_feat definition",
    "type":  "object",
    "$id":  "https://ttrpg-converter.com/schemas/crates/ttrpg-core/schemas/pathbuilder/custom-feat.json",
    "$schema":  "https://json-schema.org/draft-07/schema#",
    "properties":  {
                       "databaseID":  {
                                          "type":  "integer",
                                          "description":  "Database identifier"
                                      },
                       "timestamp":  {
                                         "type":  "string",
                                         "pattern":  "^\\d+$",
                                         "description":  "Creation timestamp"
                                     },
                       "id":  {
                                  "type":  "string",
                                  "pattern":  "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
                                  "description":  "Unique feat ID (UUID)"
                              },
                       "name":  {
                                    "type":  "string",
                                    "minLength":  1,
                                    "description":  "Feat name"
                                },
                       "level":  {
                                     "type":  "integer",
                                     "minimum":  1,
                                     "maximum":  20,
                                     "description":  "Required level for feat"
                                 },
                       "traits":  {
                                      "type":  "string",
                                      "description":  "Feat traits (comma-separated)"
                                  },
                       "prerequisites":  {
                                             "type":  "string",
                                             "description":  "Feat prerequisites"
                                         },
                       "frequency":  {
                                         "type":  "string",
                                         "description":  "Usage frequency"
                                     },
                       "trigger":  {
                                       "type":  "string",
                                       "description":  "Trigger condition"
                                   },
                       "description":  {
                                           "type":  "string",
                                           "minLength":  1,
                                           "description":  "Feat description and effects"
                                       },
                       "src":  {
                                   "type":  "string",
                                   "description":  "Source book/pack"
                               },
                       "actionType":  {
                                          "type":  "string",
                                          "enum":  [
                                                       "",
                                                       "1",
                                                       "2",
                                                       "3",
                                                       "R",
                                                       "F"
                                                   ],
                                          "description":  "Action type (empty=passive, 1-3=actions, R=reaction, F=free)"
                                      }
                   },
    "title":  "Custom Feat",
    "required":  [
                     "databaseID",
                     "timestamp",
                     "id",
                     "name",
                     "level",
                     "traits",
                     "description",
                     "src"
                 ]
}
